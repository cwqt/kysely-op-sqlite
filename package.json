    "access": "public"
  },
  "peerDependencies": {
    "@op-engineering/op-sqlite": ">=9.0.0",
    "kysely": ">=0.27.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@op-engineering/op-sqlite": "^15.2.5",
    "@types/react": "^19.0.0",
    "kysely": "^0.27.0",
    "react": "^19.0.0",
    "typescript": "~5.8.3"
