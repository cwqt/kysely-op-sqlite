{
  "name": "kysely-op-sqlite",
  "version": "0.1.0",
  "description": "Kysely dialect for @op-engineering/op-sqlite",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
